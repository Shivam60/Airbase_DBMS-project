<Window x:Class="airbase.country_view"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:airbase"
        mc:Ignorable="d"
        Title="country_view" Height="332" Width="419"
        Loaded="Window_Loaded">
    <Grid>
        <Label x:Name="label" Content="Country View" HorizontalAlignment="Left" Margin="150,5,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.526,-0.385" Width="155"/>
        <DataGrid x:Name="dataGrid" ItemsSource="{Binding LoadDataBinding}" IsSynchronizedWithCurrentItem="True"  Margin="10,36,0,0" VerticalAlignment="Top" Height="204" Width="391"/>
        <Button x:Name="button" Content="Back" HorizontalAlignment="Left" Margin="10,256,0,0" VerticalAlignment="Top" Width="75" Height="35" Click="button_Click"/>
        <Button x:Name="button1" Content="Exit" HorizontalAlignment="Left" Margin="326,256,0,0" VerticalAlignment="Top" Width="75" Height="35" Click="button1_Click"/>

    </Grid>
</Window>
